[general]

#log = "~/.subssh/error.log"

## subssh "binary" location
#subssh_bin = /usr/bin/subssh

## Hostname display on eg. on promt.
## Defaults to system hostname.
#display_hostname = vcs.linkkijkl.fi

[app:subssh.apps.vcs.svn]
#repositories = ~/.subssh/repos/svn
#webview = ~/.subssh/repos/websvn
urls = Read/Write|svn+ssh://$hostname/$name_on_fs
       Webview|http://$hostname/websvn/$name_on_fs

[app:subssh.apps.vcs.git]
#repositories = ~/.subssh/repos/svn
#webview = ~/.subssh/repos/websvn
urls = Read/Write|ssh://$hostname/$name_on_fs
       Read only clone|http://$hostname/repo/$name_on_fs
       Webview|http://$hostname/gitphp/$name_on_fs


[app:subssh.apps.uptime]
hello = config hello


[app:subssh.apps.whoami]

[app:subssh.apps.pubkeymanager]
